{
  "name": "@hellhub-collective/cli",
  "license": "MIT",
  "private": false,
  "description": "The official CLI for HellHub API. Stay updated about the current war right without leaving your terminal.",
  "version": "1.0.0",
  "main": "build/index.mjs",
  "types": "build/index.d.ts",
  "keywords": [
    "hellhub",
    "cli",
    "typescript",
    "helldivers 2",
    "community"
  ],
  "files": [
    "build"
  ],
  "bin": {
    "hellhub": "build/index.mjs"
  },
  "scripts": {
    "test": "bun test",
    "clean": "rm -rf build",
    "prepublishOnly": "bun run build",
    "copy": "bun run scripts/copy-assets.ts",
    "build-script": "bun run scripts/build.ts",
    "build": "bun run clean && bun run build-script && bun run copy",
    "format": "prettier . --write",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/accounting": "^0.4.5",
    "@types/boxen": "^3.0.1",
    "@types/chalk": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "bun-types": "^1.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hellhub-collective/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/hellhub-collective/sdk/issues"
  },
  "author": {
    "name": "Fabio Nettis",
    "email": "nettisfabio@gmail.com"
  },
  "dependencies": {
    "@hellhub-collective/sdk": "^1.0.5",
    "accounting": "^0.4.1",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.4",
    "commander": "^12.0.0"
  }
}
